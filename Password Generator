import 'dart:io';
import 'dart:math';
String generate_Password(int L){
  String C="abcdefghigklmnopqrstuvwxyz"
      "ABCDEFGHIJKLMNOPQRSTUVWXYZ"
      "0123456789"
      "!@#\$%&^*";
  Random r=Random();
  String Password="";
  for(int x=0;x<L;x++){
    int i=r.nextInt(C.length);
    Password+=C[i];
  }
  return Password;
}
main(){
  stdout.write("Enter Your Password Length :");
  int L=int.parse(stdin.readLineSync()!);
  String Password =generate_Password(L);
  print("The Password is :$Password");
}
